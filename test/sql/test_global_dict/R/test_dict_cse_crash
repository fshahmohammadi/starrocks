SELECT id, array_map(x -> array_map(y -> upper(str_col), arr2), arr1) FROM t1 ORDER BY id;
-- result:
1	[["AAA","AAA"],["AAA","AAA"]]
2	[["BBB","BBB"],["BBB","BBB"]]
-- !result
SELECT l.id, array_map(x -> array_map(y -> upper(l.str_col), l.arr2), l.arr1)
FROM t1 l JOIN t2 r ON l.id = r.id ORDER BY l.id;
-- result:
1	[["AAA","AAA"],["AAA","AAA"]]
2	[["BBB","BBB"],["BBB","BBB"]]
-- !result
SELECT l.id, array_map(x -> array_map(y -> murmur_hash3_32(l.str_col), l.arr2), l.arr1)
